* {
    background-color: rgba(50, 50, 50, 0.25);
    text-color: #FFFFFF;
    spacing: 0;
    width: 512px;
    height: 512px;
}

inputbar {
    children: [entry];
}

entry {
    padding: 16px;
}

listview {
    cycle: false;
    scrollbar: false;
}

element {
    padding: 16px;
}

element selected {
    background-color: rgba(255, 255, 255, 0.1);
}
